scanf("%c",&key);
time_t start_time, end_time;

int main(void)
{
get_name();

    system("clear");
	printf("Hello %s",name);
	printf("\n");

key_t() // t누르면 게임순위(정렬기능 포함)를 보여주는 사용자설정 함수.

//( mapprint1() 함수 들어갈 자리. )

time(&start_time)

//( if (mapprint5()후 클리어 완료) 자리. )

time(&end_time)
double difftime(end_time, start_time);
d_diff = difftime(end_time, start_time);
//'이름+기록'을 정렬하지 않고 우선 ranking.txt파일에 저장.

  FILE *out;
  out = fopen("ranking.txt","w");
  fprintf(out,"%s %f\n",name,d_diff);
  close(out);

sort();

return 0;
}

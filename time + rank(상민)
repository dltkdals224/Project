//정렬함수
void sort()
{
  int i,j,tmp;
  char rank_name[100];
  int rank_time[100];

	rank_name[a]=rank_time[a];
  FILE *in;
  int data;
  in = fopen("ranking.txt","r");
  for(i=0;i<100,i++)
    scanf(in,"%s %d\n",rank_name[a],rank_time[a])

*/ 시발 이름이랑 기록을 어떻게 같이 기록하는가?
		// 시간 순위 정렬
  for(i=0;i<100;i++)
    for(j=100;j>i;j--)
      if(rank_time[j-1]>rank_time[j]){
        tmp = ramk_time[j-1];
        rank_time[j-1]=rank_time[j];
        rank_time[j]=tmp;
      }
		// 이름 순위( 이름은 순위가 없으므로 어떻게 배열해야 할까?? ) 씨발씨발
    for(i=0;i<100;i++)
      for(j=100;j>i;j--)
        if(rank_name[j-1]>rank_name[j]){
          tmp = ramk_name[j-1];
          rank_name[j-1]=rank_name[j];
          rank_name[j]=tmp;
      }

	*/ 시발 이름이랑 기록을 어떻게 같이 기록하는가?
        key_t()
      return 0;
}

//key_t 함수
void key_t()
{
  int i;
  if (key=='t')
  sort()
  for(i=0;i<5;i++)
  printf("rank %d. %s's recode = %f\n",i+1,rank_name[i],rank_time[i]);
  return 0;
}

//이름 입력 함수
void get_name()
{
char name[M] = " ";
	int i=0,a;
	printf("Start....\n input name : ");
	while(1){
			a = getchar();
			if(i==M){
					while((a=getchar()) != '\n')
							;
							printf("1자 이상 10자 이하의 이름을 입력해 주세요 : ");
							i = 0;
							continue;
	}
			if((a =='\n')||(a == EOF)) {
					name[i++] = '\0';
					break;
	}
			name[i++] = a;
}
return 0;
}
